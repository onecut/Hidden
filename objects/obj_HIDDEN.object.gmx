if place_meeting(x,y+vsp,par_wall)
{
while (!place_meeting(x,y+sign(vsp),par_wall)) y +=sign(vsp);
if (sign(vsp) == 1) 
    {
    grounded = 1;
    djump =1;
    }
vsp = 0;

}
else
{
grounded = 0;
}

