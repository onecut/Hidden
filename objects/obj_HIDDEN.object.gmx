image_xscale = -1;
facing_droite = 0;
}

if (Key_Right)
{
hsp = 2.3;
image_xscale = 1;
facing_droite = 1;
}

//neutral input = 0 movement
if ((Key_Right &amp;&amp; Key_Left) or (!Key_Right &amp;&amp; !Key_Left))
{
hsp = 0;
}

//Attaque TEST



